{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% if object.favoriteCities %}
        {% for city in object.favoriteCities %}
            {{ get_city_name_from_insee_code(city) }} ({{ city }})
            {% if not loop.last %}, {% endif %}
        {% endfor %}
    {% endif %}
{% endblock %}
